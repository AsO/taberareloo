{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [ "lib/Base.js", "lib/DOM.js", "lib/Async.js", "lib/proto.js", "lib/utils.js", "lib/repository.js", "lib/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
     "default_icon" : "skin/fork19.png",
     "default_title": "taberareloo",
     "popup"        : "popup.html"
   },
   "options_page": "options.html",
   "icons": {
     "128": "skin/fork128.png",
     "64": "skin/fork64.png",
     "32": "skin/fork32.png",
     "16": "skin/fork16.png"
   },
   "description": "Yet Another Tombloo on Chromium",
   "name": "Taberareloo",
   "permissions": [ "http://*/*", "https://*/*", "tabs" ],
   "version": "0.0.9"
}
